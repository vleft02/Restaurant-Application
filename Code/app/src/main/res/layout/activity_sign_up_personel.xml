<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_minimalist"
    tools:context="gr.aueb.softeng.view.Owner.HomePage.SignUp.SignUpPersonel.SignUpPersonelActivity">

    <TextView
        android:id="@+id/Guide"
        android:layout_width="247dp"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:text="Personel Registration"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/CustomerNameText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="405dp"
        android:layout_height="678dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/createAccButton"
            android:layout_width="129dp"
            android:layout_height="52dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/button_gradient2"
            android:text="@string/create_account"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefTelephoneText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/telephone"
            android:inputType="phone"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/ChefIbanText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefNameText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/Name"
            android:inputType="text"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/ChefSurnameText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/Surname"
            android:inputType="text"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/ChefUsernameText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefIbanText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/iban"
            android:inputType="number"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/ChefTinText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefTinText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/tin"
            android:inputType="text"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/ChefPasswordText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefPasswordText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="24dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/Password"
            android:inputType="textPassword"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/createAccButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefEmailText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/email"
            android:inputType="textEmailAddress"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/ChefTelephoneText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.085"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ChefUsernameText"
            android:layout_width="230dp"
            android:layout_height="62dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/textrect_field2"
            android:ems="10"
            android:hint="@string/username"
            android:inputType="text"
            android:minHeight="48dp"
            android:textColor="#FFFFFF"
            android:textColorHint="#BEB9B9"
            app:layout_constraintBottom_toTopOf="@+id/ChefEmailText"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/gobackButton3"
        android:layout_width="52dp"
        android:layout_height="55dp"
        android:background="@drawable/left_arrow"
        android:text="Go Back"
        android:textAllCaps="false"
        android:textColor="#00FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>